{
    "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
    "tasks.max": "1",
    "database.hostname": "currentaccounts-db",
    "database.port": "5432",
    "database.user": "currentaccounts",
    "database.password": "currentaccounts",
    "database.dbname": "currentaccounts",
    "database.server.name": "currentaccounts-db",
    "schema.include.list": "currentaccounts",
    "table.include.list": "currentaccounts.outboxevent",
    "tombstones.on.delete": "false",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.storage.StringConverter",
    "transforms": "saga",
    "transforms.saga.type": "io.debezium.transforms.outbox.EventRouter",
    "transforms.saga.route.topic.replacement": "${routedByValue}.request",
    "poll.interval.ms": "100"
}